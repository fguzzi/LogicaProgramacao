 algoritmo "idade"

 var

    idade:inteiro

 inicio
 
    escreval("Digite sua idade?")
    leia(idade)
    
        se(idade >= 67) entao
    
        escreval("Parabéns, você já pode ser vacinado(a)")

        senao
        
        escreval("Aguarde sua vez para ser vacinado(a)")

        fimse
        
    escreval("Obrigado por acessar o sistema")

 fimalgoritmo