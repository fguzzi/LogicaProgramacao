algoritmo "Exercio14desafio"

var

    a, b, c:inteiro
    soma:real
    
inicio

     escreval("Digite o valor A: ")
     leia(a)
     escreval("Digite o valor B: ")
     leia(b)
     escreval("Digite o valor C: ")
     leia(c)
     
     soma <- exp((a+b+c),2)
     
     escreval("Valor do quadrado da soma: ",soma)
     


fimalgoritmo