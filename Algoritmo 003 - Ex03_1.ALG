algoritmo "EX03_1"
var

   n1, n2, multiplicacao, soma, subtracao: inteiro

inicio

   escreva("Digite um numero: ")
   leia(n1)
   
   escreva("Digite um numero: ")
   leia(n2)
   
   soma <- n1 + n2
   subtracao <- n1 - n2
   multiplicacao <- n1 * n2
   
   escreval ("Soma = ", soma)
   escreval ("Subtracao = ", subtracao)
   escreval ("Multiplicacao = ", multiplicacao)
   

fimalgoritmo