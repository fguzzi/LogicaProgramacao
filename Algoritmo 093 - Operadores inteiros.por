programa
{
	
	funcao inicio()
	{
		inteiro a = 5, b = 2
		inteiro soma, subt, mult, div, resto

		soma = a + b
		subt = a - b
		mult = a * b
		div = a / b
		resto = a % b

		escreva("\nSoma = ",soma)
		escreva("\nSubtração = " + subt)
		escreva("\nMultiplicação = ", mult)
		escreva("\nDivisão = " + div)
		escreva("\nResto Divisão = ", resto, "\n")
	}
}
