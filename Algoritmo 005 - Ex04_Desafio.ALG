algoritmo "desafioex04"

var

  a, b, c, result:inteiro

inicio

  escreval("Digite um numero= ")
  leia(a)
  
  escreval("Digite um numero= ")
  leia(b)
  
  escreval("Digite um numero= ")
  leia(c)
  
  result<- a + b + c
  escreval ("Soma= ", result)
  
  result<- a - b - c
  escreval ("Subtracao= ", result)
  
  result<- a * b * c
  escreval ("Multiplicacao= ", result)

fimalgoritmo