algoritmo "EX03_2"
var

   n1, n2, total: inteiro

inicio

   escreva("Digite um numero: ")
   leia(n1)
   
   escreva("Digite um numero: ")
   leia(n2)
   
   total <- n1 + n2
   escreval ("Soma = ", total)
   
   total <- n1 - n2
   escreval ("Subtracao = ", total)
   
   total <- n1 * n2
   escreval ("Multiplicacao = ", total)

fimalgoritmo